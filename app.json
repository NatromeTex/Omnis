{
  "expo": {
    "name": "Omnis",
    "slug": "Omnis",
    "version": "0.9.0-alpha",
    "orientation": "portrait",
    "icon": "./engine/assets/icon.png",
    "scheme": "omnis",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omnis.app"
    },
    "android": {
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.omnis.app"
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
